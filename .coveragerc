[run]
omit =
    */python?.?/*
    */lib-python/?.?/*.py
    */lib_pypy/_*.py
    */site-packages/ordereddict.py
    */site-packages/nose/*
    */unittest2/*
    *mc/templates/docker/etc/adsws/common.py.monkeypatch
    *mc/templates/docker/etc/gunicorn/gunicorn.conf.py

exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
